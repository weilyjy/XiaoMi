html,body,p,div,ul,li,ol,dl,dt,dd,table,tr,td,form,h1,h2,h3,h4,h5,h6,img,input,hr{
	margin: 0;
	padding: 0;
}
body{
    background:#f9f9f9;
}
#wtf_body{
	width: 860px;
	height: 520px;
	background: #fff;
	margin: 0 auto;
	margin-top: 15px;
	overflow: hidden;
}
#wtf_banner_logo{
	background: #ff5900;
	height: 48px;
	width: 48px;
	margin: 0 auto;
	text-align: center;
	margin-bottom: 40px;
}
#wtf_banner_logo i{
	font-size: 30px;
	color: #fff;
	line-height: 48px;
	
}
#wtf_banner_title{
	width: 786px;
	height: 45px;
	margin:0 auto;
}
#wtf_banner_title h1{
	font-weight: normal;
	font-size: 30px;
	color: #333;
    line-height: 1.5;
    text-align: center;
    margin:0 auto;
}
#wtf_body_wai{
	width: 786px;
	height: 356px;
	margin:0 auto;
}
