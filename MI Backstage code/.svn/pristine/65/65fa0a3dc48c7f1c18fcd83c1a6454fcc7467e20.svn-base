* {
  margin: 0;
  padding: 0; }

li {
  list-style: none; }

a {
  text-decoration: none;
  color: #000; }

#wr_nav1 {
  width: 100%;
  height: 40px;
  background-color: #333333; }
  #wr_nav1 .wr_nav1_ul1, #wr_nav1 .wr_nav1_ul2 {
    margin-left: 20px;
    float: left; }
    #wr_nav1 .wr_nav1_ul1 li, #wr_nav1 .wr_nav1_ul2 li {
      float: left;
      line-height: 40px; }
      #wr_nav1 .wr_nav1_ul1 li a, #wr_nav1 .wr_nav1_ul2 li a {
        color: #B0B0B0;
        border-right: 1px solid #3B4040;
        padding: 0px 8px;
        font-size: 12px; }
      #wr_nav1 .wr_nav1_ul1 li a:hover, #wr_nav1 .wr_nav1_ul2 li a:hover {
        color: #fff; }
    #wr_nav1 .wr_nav1_ul1 li:nth-of-type(11) a, #wr_nav1 .wr_nav1_ul2 li:nth-of-type(11) a {
      border: 0px; }
  #wr_nav1 .wr_nav1_ul2 {
    margin-right: 20px;
    float: right; }
    #wr_nav1 .wr_nav1_ul2 li:nth-of-type(3) a {
      border: 0px; }
    #wr_nav1 .wr_nav1_ul2 li:nth-of-type(4) a {
      display: inline-block;
      width: 120px;
      height: 38px;
      background-color: #424242;
      text-align: center;
      border: 0px; }
    #wr_nav1 .wr_nav1_ul2 li:nth-of-type(4) a:hover {
      color: #ff6700; }

#wr_nav2 {
  height: 100px;
  width: 100%;
  text-align: center;
  position: relative; }
  #wr_nav2 .logo {
    position: absolute;
    left: 20px;
    margin-left: 20px;
    background: url(/images/logo.png) no-repeat;
    display: inline-block;
    width: 55px;
    height: 55px;
    top: 20px; }
  #wr_nav2 .wr_nav2_ul {
    position: absolute;
    left: 200px; }
    #wr_nav2 .wr_nav2_ul li {
      line-height: 100px;
      float: left; }
      #wr_nav2 .wr_nav2_ul li a {
        font-size: 16px;
        padding: 12px 0 0 30px;
        color: #333; }
      #wr_nav2 .wr_nav2_ul li a:hover {
        color: #ff6700; }
  #wr_nav2 .wr_seach {
    width: 296px;
    border: 1px solid #E0E0E0;
    position: absolute;
    right: 20px;
    top: 25px; }
    #wr_nav2 .wr_seach form {
      overflow: hidden; }
      #wr_nav2 .wr_seach form input {
        height: 50px;
        float: left;
        outline: none;
        border: none; }
      #wr_nav2 .wr_seach form input:nth-of-type(1) {
        margin-left: 10px;
        width: 240px;
        border-right: 1px solid #E0E0E0; }
      #wr_nav2 .wr_seach form i {
        font-size: 20px;
        line-height: 50px;
        text-align: center; }
    #wr_nav2 .wr_seach .defalutKey {
      position: absolute;
      top: 12px;
      left: 0; }
      #wr_nav2 .wr_seach .defalutKey a {
        color: #757575;
        background: #EEEEEE;
        font-size: 12px; }
      #wr_nav2 .wr_seach .defalutKey a:nth-of-type(1) {
        margin-left: 50px; }
      #wr_nav2 .wr_seach .defalutKey a:hover {
        background: #ff6700;
        color: #fff; }
    #wr_nav2 .wr_seach .keyWorld {
      position: absolute;
      left: 0;
      top: 50;
      z-index: 10;
      display: none; }
      #wr_nav2 .wr_seach .keyWorld ul {
        border: 1px solid #FF6700;
        width: 248px;
        background: #fff; }
        #wr_nav2 .wr_seach .keyWorld ul li {
          position: realtive;
          text-align: left;
          height: 30px; }
          #wr_nav2 .wr_seach .keyWorld ul li a {
            padding: 0px 15px;
            font-size: 12px;
            color: #424242; }
          #wr_nav2 .wr_seach .keyWorld ul li span {
            position: absolute;
            right: 10px;
            font-size: 12px;
            color: #b0b0b0; }

#wr_banner .play {
  margin-left: 20px;
  width: 100%;
  height: 460px;
  overflow: hidden;
  position: relative;
  top: -9px; }
#wr_banner ol {
  position: absolute;
  right: 5%;
  bottom: 5px;
  z-index: 20; }
#wr_banner ol li {
  float: left;
  margin-right: 3px;
  display: inline;
  cursor: pointer;
  background: #666;
  border: 2px solid #757575;
  padding: 2px;
  color: #757575;
  border-radius: 100%;
  width: 3px;
  height: 3px; }
#wr_banner .active {
  padding: 3px;
  font-weight: bold;
  color: #ffffff;
  background: #fff;
  position: relative;
  bottom: 2px; }
#wr_banner .imgList {
  height: 460px;
  width: 1226px;
  z-index: 10;
  display: block; }
#wr_banner .imgOn {
  display: block; }
#wr_banner .imgList li {
  display: none; }
#wr_banner .imgList li img {
  width: 100%;
  height: 460px; }
#wr_banner .prive, #wr_banner .next {
  position: absolute;
  width: 30px;
  height: 60px;
  line-height: 60px;
  text-align: center;
  color: #EFEFEF;
  cursor: pointer;
  background-color: rgba(0, 0, 0, 0); }
#wr_banner .prive {
  top: 50%;
  left: 270px; }
#wr_banner .prive:hover {
  background-color: rgba(0, 0, 0, 0.5); }
#wr_banner .next:hover {
  background-color: rgba(0, 0, 0, 0.5); }
#wr_banner .prive strong, #wr_banner .next strong {
  color: #EFEFEF;
  font-size: 25px; }
#wr_banner .next {
  top: 50%;
  right: 53px; }

#wr_sideNav {
  position: absolute;
  top: 131px;
  left: 20px;
  background: rgba(0, 0, 0, 0.6);
  width: 244px;
  height: 460px;
  z-index: 10; }

#wr_sideNav .sideNav_good {
  width: 244px;
  height: 460px;
  position: absolute;
  left: 0px;
  top: 0px; }

#wr_sideNav .sideNav_good li {
  position: relative;
  width: 244px;
  text-align: center;
  height: 42px;
  line-height: 42px;
  display: block; }

#wr_sideNav .sideNav_good li:nth-of-type(1) {
  margin-top: 20px; }

#wr_sideNav .sideNav_good li a {
  width: 224px;
  height: 42px;
  margin-left: 15px;
  font-size: 14px;
  display: inline-block;
  color: #fff;
  text-align: left; }

#wr_sideNav .sideNav_good li span {
  color: #9B9B90;
  position: absolute;
  font-size: 16px;
  right: 10px; }

#wr_sideNav .sideNav_good li:hover {
  background: #FF6700; }

#wr_sideNav .sideNav_good li .sideNav_hideGoods {
  width: 373px;
  height: 460px;
  position: absolute;
  left: 244px;
  top: 0px;
  background-color: white;
  display: none;
  box-shadow: 1px 1px 2px rgba(0, 0, 0, 0.1); }

#wr_sideNav .sideNav_good li .sideNav_hideGoods .box img {
  width: 60px;
  height: 70px; }

#wr_sideNav .sideNav_good li .sideNav_hideGoods .box span {
  width: 100px;
  display: inline-block;
  font-size: 10px; }

#wr_sideNav .sideNav_good li .sideNav_hideGoods .box {
  float: left;
  width: 180px;
  height: 70px; }

#wr_sideNav .sideNav_good li .sideNav_hideGoods .title p {
  width: 371px;
  height: 30px;
  margin-top: 5px; }

#wr_sideNav .sideNav_good li .sideNav_hideGoods .title {
  width: 373px;
  height: 50px;
  border-bottom: 1px solid #F4F3F3;
  overflow: hidden; }